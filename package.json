{
  "name": "websites",
  "version": "1.0.0",
  "description": "Serve one or more websites",
  "type": "module",
  "xalias": {
    "xprocess": false
  },
  "scripts": {
    "prebuild": "rimraf .parcel-cache dist",
    "build": "parcel build src/index.html",
    "clean": "rimraf .parcel-cache dist",
    "start": "parcel src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "staticFiles": {
    "staticPath": "src/sites/hellwig.org/static",
    "staticOutPath": "static"
  },
  "repository": {
    "type": "git",
    "url": "https://fhellwig@dev.azure.com/fhellwig/hellwig.org/_git/hellwig.org"
  },
  "keywords": [
    "website"
  ],
  "author": "Frank Hellwig",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.1",
    "parcel": "^2.6.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "process": "^0.11.10"
  }
}
